<table>
  <thead>
  <tr>
    <% Span.attribute_names.each do |attribute| %>
      <th scope="col"><%= attribute.humanize %></th>
    <% end %>
  </tr>
  </thead>
  <tbody>
  <% spans.each do |span| %>
    <tr>
      <% span.attributes.each do |attribute| %>
        <td><%= attribute[1] %></td>
      <% end %>
    </tr>
  <% end %>
  </tbody>
</table>
